{
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true,
  },
  "extends": "airbnb",
  "rules": {
    "no-console": 1,
    "no-unused-vars": 1,
    "no-tabs": 0,
    "indent": [2, "tab"],
    "react/jsx-indent": [2, "tab"],
    "react/jsx-indent-props": [2, "tab"],
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0,
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": false
  }
}